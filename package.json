{
    "name": "focus-core",
    "version": "2.1.1",
    "description": "Focus library core part.",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/KleeGroup/focus-core.git"
    },
    "keywords": [
        "spa",
        "react",
        "es6"
    ],
    "author": "focus@kleegroup.com",
    "documentation": "http://kleegroup.github.io/focus-docs/",
    "license": "MIT",
    "homepage": "https://github.com/KleeGroup/focus-core",
    "bugs": {
        "url": "https://github.com/KleeGroup/focus-core/issues"
    },
    "scripts": {
        "babelify": "better-npm-run babelify",
        "bundle": "better-npm-run bundle",
        "watch": "better-npm-run watch",
        "test": "mocha src/**/__tests__/**/*.js",
        "test:watch": "mocha src/**/__tests__/**/*.js -w",
        "build": "better-npm-run babelify && better-npm-run bundle"
    },
    "betterScripts": {
        "babelify": {
            "command": "node scripts/babelify.js"
        },
        "bundle": {
            "command": "webpack --color --progress",
            "env": {
                "DEV": false,
                "NODE_ENV": "production",
                "LIBRARY_NAME": "FocusCore",
                "MINIMIFY": false,
                "PACKAGE_JSON_PATH": "../"
            }
        },
        "watch": {
            "command": "webpack --color --progress --watch",
            "env": {
                "DEV": false,
                "NODE_ENV": "production",
                "LIBRARY_NAME": "FocusCore",
                "MINIMIFY": false,
                "PACKAGE_JSON_PATH": "../"
            }
        }
    },
    "dependencies": {
        "@types/flux": "0.0.34",
        "@types/node": "6.0.58",
        "flux": "3.1.2",
        "i18next": "4.1.4",
        "immutable": "3.8.1",
        "keymirror": "0.1.1",
        "lodash": "4.17.4",
        "object-assign": "4.1.0",
        "uuid": "3.0.1"
    },
    "devDependencies": {
        "babel-preset-focus": "git://github.com/KleeGroup/babel-preset-focus.git#refreshing-updating",
        "better-npm-run": "0.0.13",
        "chai": "3.5.0",
        "chai-subset": "1.4.0",
        "eslint-config-focus": "git://github.com/KleeGroup/eslint-config-focus.git#updating-refreshing",
        "mocha": "3.2.0",
        "moment": "2.17.1",
        "numeral": "1.5.6",
        "react": "15.4.2",
        "react-addons-test-utils": "15.4.2",
        "react-dom": "15.4.2",
        "webpack-focus": "git://github.com/KleeGroup/webpack-focus.git#builder-archetype"
    }
}